{
    "name": "reactium",
    "version": "2.3.14",
    "description": "A framework for creating React + Redux apps using the domain driven design (DDD) paradigm.",
    "main": "index.js",
    "scripts": {
        "start": "node build/.core/index.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "manifest": "node ./.core/createManifest.js",
        "build": "npm install --production-only",
        "build:gulp": "cross-env NODE_ENV=production gulp",
        "build:babel-core": "cross-env NODE_ENV=production babel .core --out-dir build/.core",
        "build:babel-src": "cross-env NODE_ENV=production babel src --out-dir build/src",
        "static": "npm-run-all build:* && gulp static",
        "electron": "cross-env NODE_ENV=production PORT=7000 electron build/electron",
        "local": "gulp local",
        "local:ssr": "gulp local:ssr",
        "clean": "gulp clean",
        "docker": "npm-run-all -s clean docker:*",
        "docker:build": "cross-env docker image build -t $npm_package_name:$npm_package_version -t $npm_package_name:latest .",
        "docker:push": "cross-env docker push $npm_package_name:$npm_package_version && cross-env docker push $npm_package_name:$npm_package_version:latest",
        "deploy": "cf push",
        "precommit": "lint-staged",
        "postinstall": "npm-run-all -s build:*"
    },
    "keywords": [
        "react",
        "redux",
        "framework",
        "javascript",
        "ddd"
    ],
    "author": "Cam Tullos <cam@tullos.ninja> (http://cam.tullos.ninja) and John Dillick <john@dillick.us>",
    "license": "MIT",
    "engines": {
        "node": "9.2.1",
        "npm": ">=5.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Atomic-Reactor/Reactium.git"
    },
    "browser": {
        "parse/node": false
    },
    "dependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/node": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "ajv": "^6.5.2",
        "axios": "^0.19.0",
        "body-parser": "^1.18.3",
        "cookie-parser": "^1.4.3",
        "cookie-session": "^2.0.0-beta.3",
        "copy-to-clipboard": "^3.0.8",
        "core-js": "^3.4.7",
        "cors": "^2.8.5",
        "cross-env": "^6.0.3",
        "directory-tree": "^2.0.0",
        "express": "^4.16.2",
        "express-http-proxy": "^1.1.0",
        "globby": "^10.0.1",
        "gsap": "^3.0.1",
        "htmltojsx": "^0.3.0",
        "http-auth": "^3.2.3",
        "js-beautify": "^1.7.5",
        "marked": "^0.7.0",
        "moment": "^2.20.1",
        "morgan": "^1.9.1",
        "npm-run-all": "^4.1.5",
        "object-path": "^0.11.4",
        "parse": "^2.10.0",
        "prettier": "^1.15.1",
        "prop-types": "^15.6.2",
        "querystring-browser": "^1.0.4",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-frame-component": "^4.0.1",
        "react-helmet": "^5.2.0",
        "react-redux": "^7.1.3",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^5.1.2",
        "react-syntax-highlighter": "^11.0.2",
        "redbox-react": "^1.6.0",
        "redux": "^4.0.0",
        "redux-devtools": "^3.4.1",
        "redux-devtools-dock-monitor": "^1.1.2",
        "redux-devtools-log-monitor": "^1.4.0",
        "redux-local-persist": "0.1.0",
        "redux-super-thunk": "0.0.8",
        "semver": "^6.3.0",
        "serialize-javascript": "^2.1.1",
        "underscore": "*",
        "xss": "^1.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "atomic-reactor-cli": "^2.0.21",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "babel-plugin-module-resolver": "^3.1.1",
        "browser-sync": "^2.26.3",
        "camelcase": "^5.0.0",
        "chalk": "^3.0.0",
        "cli-spinners": "^2.2.0",
        "decamelize": "^3.2.0",
        "del": "^5.1.0",
        "electron": "^7.1.3",
        "eslint-plugin-react": "^7.11.1",
        "find-free-port": "^2.0.0",
        "fs-extra": "^8.1.0",
        "fs-readdir-recursive": "^1.1.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-concat": "^2.6.1",
        "gulp-csso": "^4.0.1",
        "gulp-if": "^3.0.0",
        "gulp-install": "^1.1.0",
        "gulp-less": "^4.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-run": "^1.7.1",
        "gulp-run-electron": "^3.0.2",
        "gulp-sass": "^4.0.1",
        "gulp-sourcemaps": "^2.4.1",
        "gulp-util": "^3.0.8",
        "gulp-watch": "^5.0.0",
        "handlebars": "^4.0.12",
        "husky": "^3.1.0",
        "ignore-loader": "^0.1.2",
        "lint-staged": "^9.5.0",
        "node-sass-tilde-importer": "^1.0.2",
        "nodemon": "^2.0.1",
        "ora": "^4.0.3",
        "readdir-recursive": "0.0.4",
        "run-sequence": "^2.2.1",
        "slugify": "^1.3.3",
        "vinyl-source-stream": "^2.0.0",
        "webpack": "^4.16.1",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-filter-warnings-plugin": "^1.2.0",
        "webpack-hot-middleware": "^2.22.3",
        "yargs": "^15.0.2"
    },
    "nodemonConfig": {
        "watch": [
            "src/manifest.js"
        ],
        "ignore": [
            "src/app/*"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
